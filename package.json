{
  "name": "@mukea/uiohook-napi",
  "version": "1.7.0",
  "author": {
    "name": "mukea.cc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mukea-org/uiohook-napi"
  },
  "license": "MIT",
  "keywords": [
    "iohook",
    "uiohook",
    "libuiohook",
    "hook",
    "input",
    "keyboard",
    "mouse"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "install": "node-gyp-build",
    "prebuild": "prebuildify --napi",
    "prepublishOnly": "npm run build-ts && npm run prebuild",
    "build-ts": "tsc",
    "demo": "ts-node src/demo.ts",
    "make-libuiohook-patch": "git -C ./libuiohook diff --cached > ./src/libuiohook.patch",
    "apply-libuiohook-patch": "git -C ./libuiohook apply ../src/libuiohook.patch"
  },
  "files": [
    "dist",
    "binding.gyp",
    "libuiohook/src",
    "libuiohook/include",
    "src/lib",
    "prebuilds"
  ],
  "devDependencies": {
    "@types/node": "^20.11.30",
    "prebuildify": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "node-gyp-build": "4.x.x"
  },
  "prebuild": {
    "test": "dist/prebuild-test-noop.js"
  },
  "gypfile": true,
  "engines": {
    "node": ">=18"
  },
  "napi_versions": [
    8,
    9,
    10
  ],
  "overrides": {
    "node-gyp": "^10.0.0"
  },
  "resolutions": {
    "node-gyp": "^10.0.0"
  },
  "pnpm": {
    "overrides": {
      "node-gyp": "^10.0.0"
    }
  }
}
